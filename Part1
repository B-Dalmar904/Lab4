#include <msp430.h>
int main(void)
{
    WDTCTL = WDT_ADLY_1000;                      // WDT 1000ms, SMCLK, interval timer
    P6OUT &= ~BIT6;                              // Changed to Bit 6 for green
    P6DIR |= BIT6;                               // Changed to Bit 6 for green
    PM5CTL0 &= ~LOCKLPM5;
    SFRIE1 |= WDTIE;
    _enable_interrupts();
    while (1);
}
    #pragma vector=WDT_VECTOR
    __interrupt void wdtled(void)
    {
            P6OUT^=BIT6;                        // Changed to Bit 6 for green
    }
